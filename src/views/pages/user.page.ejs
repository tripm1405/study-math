<button onClick="upsert('<%= data?.newId %>')">Táº¡o</button>
<table>
    <thead>
    <tr>
        <th></th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    <% for (const user of (data?.users || [])) { %>
        <tr>
            <td><%= user?.code %></td>
            <td>
                <button onClick="upsert('<%= user?._id %>')">CHI TIET</button>
                <button onClick="del('<%= user?._id %>')">XOA</button>
            </td>
        </tr>
    <% } %>
    </tbody>
</table>

<script>
    function upsert(id) {
        window.location.href = `/users/${id}`;
    }
    async function del(id) {
        await axios.delete(`http://localhost:5500/users/${id}`);
        location.reload()
    }
</script>